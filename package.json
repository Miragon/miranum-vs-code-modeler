{
  "name": "vs-code-bpmn-modeler",
  "displayName": "DigiWF: BPMN Modeler",
  "description": "Edit bpmn files inside VS Code.",
  "license": "Apache License 2.0",
  "version": "0.1.0",
  "publisher": "miragon-gmbh",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlowSquad/vs-code-bpmn-modeler"
  },
  "bugs": {
    "url": "https://github.com/FlowSquad/vs-code-bpmn-modeler/issues"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "icon": "images/bpmnModeler_logo.png",
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:bpmn-modeler"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "id": "bpmn-modeler",
        "viewType": "bpmn-modeler",
        "displayName": "BPMN Modeler",
        "selector": [
          {
            "filenamePattern": "*.bpmn"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsup src/extension.ts --dts --external=vscode && vite build --config ./web/vite.config.js",
    "web": "vite build --config web/vite.config.js",
    "web-dev": "vite build --config web/vite.config.js --watch",
    "ext": "tsup src/extension.ts --dts --external=vscode --sourcemap",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "@bpmn-io/element-template-chooser": "^0.1.0",
    "@bpmn-io/properties-panel": "^0.23.0",
    "bpmn-js": "^10.2.0",
    "bpmn-js-properties-panel": "^1.9.0",
    "camunda-bpmn-js-behaviors": "^0.3.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "jquery": "^3.6.1",
    "min-dash": "^4.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "tsup": "^6.2.3",
    "typescript": "^4.5.5",
    "vite": "^3.1.5",
    "vite-plugin-static-copy": "^0.7.0"
  }
}
